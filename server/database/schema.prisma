generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  id           Int       @default(autoincrement()) @id
  name         String
  email        String    @unique
  password     String
  created_on   DateTime? @default(now())
  updated_on   DateTime? @default(now())
  confirmed_on DateTime?
  confirmed    Boolean?  @default(false)
}
